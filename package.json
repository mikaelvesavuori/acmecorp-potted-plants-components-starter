{
  "name": "acmecorp-potted-plants-components-starter",
  "version": "1.0.2",
  "description": "React component library with components generated and/or composed with Figmagic. Made for The newline Guide to React Component Design Systems with Figmagic.",
  "keywords": [
    "acmecorp",
    "component-library",
    "figmagic",
    "newline",
    "course-material"
  ],
  "author": "Mikael Vesavuori",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx webpack",
    "init": "npm install --force && rm -rf src/stories",
    "prepublishOnly": "npm run figmagic && npm run build",
    "figmagic:init": "npx figmagic init",
    "figmagic": "npx figmagic",
    "start": "start-storybook -p 6006",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006"
  },
  "resolutions": {
    "@storybook/{app}/webpack": "^5"
  },
  "peerDependencies": {
    "prop-types": "15",
    "react": "17",
    "react-dom": "17",
    "styled-components": "5"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@babel/preset-env": "7",
    "@babel/preset-react": "7",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/blocks": "7",
    "@storybook/builder-webpack4": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack4": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/react": "6.5.12",
    "@storybook/react-webpack5": "7",
    "@storybook/testing-library": "0.0.13",
    "babel-loader": "9",
    "babel-plugin-module-resolver": "4",
    "chromatic": "5",
    "eslint": "8",
    "eslint-plugin-storybook": "0.5.7",
    "figmagic": "4",
    "file-loader": "6",
    "prettier": "2",
    "prop-types": "15",
    "react": "17",
    "react-dom": "17",
    "storybook": "7",
    "webpack": "5",
    "webpack-cli": "4",
    "webpack-dev-server": "3"
  }
}
